<?xml version="1.0" encoding="UTF-8"?>
<content xmlns:j="http://www.jahia.org/jahia/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0">
    <modules jcr:primaryType="jnt:modules">
        <portal-factory_esigate-filter j:dependencies="default serverSettings"
                                     j:modulePriority="0"
                                     j:moduleType="module"
                                     j:title="Portal factory - esigate filter"
                                     jcr:mixinTypes="jmix:hasExternalProviderExtension"
                                     jcr:primaryType="jnt:module">
                
                <portlets jcr:primaryType="jnt:portletFolder"/>
                <files jcr:primaryType="jnt:folder"/>
                <contents jcr:primaryType="jnt:contentFolder"/>
                <templates j:rootTemplatePath="/base" jcr:primaryType="jnt:templatesFolder">
                    <files jcr:primaryType="jnt:folder">
                        <Portal-Factory-icon.png j:height="16"
                                                 j:width="16"
                                                 jcr:mixinTypes="jmix:image"
                                                 jcr:primaryType="jnt:file">
                            <jcr:content jcr:mimeType="image/png" jcr:primaryType="jnt:resource"/>
                            <thumbnail jcr:mimeType="image/png" jcr:primaryType="jnt:resource"/>
                            <thumbnail2 jcr:mimeType="image/png" jcr:primaryType="jnt:resource"/>
                        </Portal-Factory-icon.png>
                        <esigate-icon.1.png j:height="16"
                                   j:width="16"
                                   jcr:mixinTypes="jmix:image"
                                   jcr:primaryType="jnt:file">
                            <jcr:content jcr:mimeType="image/png" jcr:primaryType="jnt:resource"/>
                            <thumbnail jcr:mimeType="image/png" jcr:primaryType="jnt:resource"/>
                            <thumbnail2 jcr:mimeType="image/png" jcr:primaryType="jnt:resource"/>
                        </esigate-icon.1.png>
                    </files>
                    <contents jcr:primaryType="jnt:contentFolder"/>
                    <server-settings-base j:view="serverSettings"
                                  jcr:mixinTypes="jmix:useUILocale"
                                  jcr:primaryType="jnt:template">
                        <portal-factory j:hiddenTemplate="false"
                               j:icon="/modules/portal-factory_esigate-filter/templates/files/Portal-Factory-icon.png"
                               j:titleKey="serverSettings.portalfactory"
                               jcr:mixinTypes="jmix:rbTitle"
                               jcr:primaryType="jnt:template">
                            <j:translation_en jcr:language="en"
                                    jcr:mixinTypes="mix:title"
                                    jcr:primaryType="jnt:translation"
                                    jcr:title="portal factory"/>
                        <esigate j:applyOn="jnt:globalSettings"
                        j:defaultTemplate="false"
                        j:hiddenTemplate="false"
                           j:icon="/modules/portal-factory_esigate-filter/templates/files/esigate-icon.1.png"
                           j:titleKey="serverSettings.esigateSettings"
                        j:view="serverSettings"
                           jcr:mixinTypes="jmix:rbTitle"
                        jcr:primaryType="jnt:contentTemplate">
                            <j:translation_en jcr:language="en"
                                    jcr:mixinTypes="mix:title"
                                    jcr:primaryType="jnt:translation"
                                    jcr:title="Esigate"/>
                            <pagecontent jcr:primaryType="jnt:contentList">
                                <esigate-admin jcr:primaryType="jnt:serverSettingsEsigate">
                                    <j:translation_en jcr:language="en"
                                          jcr:mixinTypes="mix:title"
                                          jcr:primaryType="jnt:translation"
                                          jcr:title="esigate admin"/>
                                </esigate-admin>
                            </pagecontent>
                        </esigate>
                        </portal-factory>
                    </server-settings-base>
                </templates>
                
            </portal-factory_esigate-filter>
    </modules>
</content>
